    function initMap() {
      var myOptions = {
        scrollwheel: false,
        //draggable: false,
        panControl: false,
        disableDefaultUI: true,
        zoomControl: true,
        zoom: window.mapData.zoom,
        center: new google.maps.LatLng(window.mapData.latitude, window.mapData.longitude),
        mapTypeId: google.maps.MapTypeId.ROADMAP,
        styles: silver
      };

      var map = new google.maps.Map(document.getElementById('map'), myOptions);

      var marker = new google.maps.Marker({
        map: map,
        position: new google.maps.LatLng(window.mapData.latitude, window.mapData.longitude),
        icon: {
            
              url: "http://localhost:4000/assets/img/marker-dark.svg",
            
            size: new google.maps.Size(28, 38),
            scaledSize: new google.maps.Size(28, 38)
          }
      });

      google.maps.event.addDomListener(window, 'resize', function() {
        map.setCenter(myOptions.center);
      });
    }
  </script>

<script async="" defer="" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBWpK890qcen1qMLVH6GUpBKegBZUXWCHo&amp;callback=initMap"></script>

